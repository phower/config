{
    "name": "phower/config",
    "description": "Simplified methods to handle configuration data in PHP.",
    "homepage": "https://github.com/phower/config",
    "issues": "https://github.com/phower/config/issues",
    "type": "library",
    "keywords": [
        "php",
        "config"
    ],
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "squizlabs/php_codesniffer": "^2.5",
        "symfony/var-dumper": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Phower\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhowerTest\\Config\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pedro Ferreira",
            "email": "pedro@phower.com"
        }
    ]
}
